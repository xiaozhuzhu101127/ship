<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="3dp"
    android:layout_marginTop="5dp"
    android:background="@drawable/gsinfobg">
    <TextView
        android:layout_width="15dp"
    	android:layout_height="17dp"
    	android:background="@drawable/numbg1"
    	android:textSize="16sp"
    	android:gravity="center"
    	android:text="1"
    	android:id="@+id/item_goods_detail_num"/>
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_toRightOf="@id/item_goods_detail_num"
    	android:layout_marginLeft="3dp"
    	android:text="林大鹏"
    	style="@style/main_style_4"
    	android:id="@+id/item_goods_detail_name"/>
    <ImageView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_below="@id/item_goods_detail_num"
    	android:layout_marginTop="3dp"
    	android:src="@drawable/gjtb"
    	android:id="@+id/item_goods_detail_bj"/>
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_toRightOf="@id/item_goods_detail_bj"
    	android:layout_below="@id/item_goods_detail_name"
    	android:layout_marginLeft="3dp"
    	android:text="5000元"
    	style="@style/item_goods_3"
    	android:id="@+id/item_goods_detail_price"/>
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_toRightOf="@id/item_goods_detail_bj"
    	android:layout_below="@id/item_goods_detail_price"
    	android:layout_marginLeft="3dp"
    	android:text="29分10秒"
    	style="@style/main_style_3"
    	android:id="@+id/item_goods_detail_time"/>
    <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentRight="true"
        android:orientation="vertical" 
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:id="@+id/item_goods_detail_trade"
        android:background="@drawable/btn27872"
        android:onClick="onClick">
       	<ImageView
       	    android:layout_width="wrap_content"
       	    android:layout_height="wrap_content"
       	    android:src="@drawable/trade"/>
       	<TextView
       	    android:layout_width="wrap_content"
       	    android:layout_height="wrap_content"
       	    android:layout_marginTop="5dp"
       	    android:text="@string/confirm"
       	    style="@style/main_style_3"/>
       	</LinearLayout>
    	<LinearLayout
	        android:layout_width="60dp"
	        android:layout_height="60dp"
	        android:layout_toLeftOf="@id/item_goods_detail_trade"
	        android:layout_marginRight="3dp"
	        android:background="@drawable/gsinfobg3"
	        android:orientation="vertical" 
	        android:gravity="center_horizontal"
	        android:padding="5dp"
	        android:id="@+id/item_goods_detail_cxcx"
	        android:onClick="onClick">
        	<ImageView
        	    android:layout_width="wrap_content"
        	    android:layout_height="wrap_content"
        	    android:src="@drawable/cxcxtb"/>
        	<TextView
        	    android:layout_width="wrap_content"
        	    android:layout_height="wrap_content"
        	    android:layout_marginTop="5dp"
        	    android:text="@string/cxcx"
        	    style="@style/main_style_3"/>
       	</LinearLayout>
       	<LinearLayout
	        android:layout_width="60dp"
	        android:layout_height="60dp"
	        android:layout_toLeftOf="@id/item_goods_detail_cxcx"
	        android:layout_marginRight="3dp"
	        android:background="@drawable/gsinfobg3"
	        android:orientation="vertical" 
	        android:gravity="center_horizontal"
	        android:padding="5dp"
	        android:id="@+id/item_goods_detail_teldriver"
	        android:onClick="onClick">
        	<ImageView
        	    android:layout_width="wrap_content"
        	    android:layout_height="wrap_content"
        	    android:src="@drawable/teltb"/>
        	<TextView
        	    android:layout_width="wrap_content"
        	    android:layout_height="wrap_content"
        	    android:layout_marginTop="5dp"
        	    android:text="@string/teldriver"
        	    style="@style/main_style_3"/>
       	</LinearLayout>
</RelativeLayout>
