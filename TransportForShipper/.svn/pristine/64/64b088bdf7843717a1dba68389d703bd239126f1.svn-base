<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/color_background">
    <include
        layout="@layout/common_title"/>
    <ScrollView 
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
		<LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="vertical"
	        android:padding="7dp">
		     <!-- 身份 -->
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:background="@drawable/publishbg"
		        android:layout_marginTop="7dp"
		        android:padding="7dp"
		        android:gravity="center_vertical">
		        <TextView
		            android:layout_width="wrap_content"
		        	android:layout_height="wrap_content"
		        	android:text="@string/yourut"
		        	style="@style/main_style_4"/>
		        
		        <TextView
		            android:layout_width="0dp"
		        	android:layout_height="wrap_content"
		        	android:drawableRight="@drawable/jt2"
		        	android:gravity="center_vertical"
		        	android:layout_weight="1"
		        	style="@style/main_style_6"
		        	android:text="@string/gsowner"/>
		    </LinearLayout>
		    <!-- 姓名 -->
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:background="@drawable/publishbg"
		        android:layout_marginTop="7dp"
		        android:padding="7dp"
		        android:gravity="center_vertical">
		        <TextView
		            android:layout_width="wrap_content"
		        	android:layout_height="wrap_content"
		        	android:text="@string/yourname"
		        	style="@style/main_style_4"/>
		        <EditText
		            android:layout_width="0dp"
		        	android:layout_height="wrap_content"
		        	android:hint="@string/select2"
		        	android:gravity="center_vertical"
		        	android:layout_weight="1"
		        	android:background="@null"
		        	style="@style/main_style_6"
		        	android:inputType="text"
		        	android:id="@+id/complete_name"/>
		    </LinearLayout>
		    <!-- 身份证号 -->
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:background="@drawable/publishbg"
		        android:layout_marginTop="7dp"
		        android:padding="7dp"
		        android:gravity="center_vertical">
		        <TextView
		            android:layout_width="wrap_content"
		        	android:layout_height="wrap_content"
		        	android:text="@string/yourno"
		        	style="@style/main_style_4"/>
		        <EditText
		            android:layout_width="0dp"
		        	android:layout_height="wrap_content"
		        	android:hint="@string/select2"
		        	android:gravity="center_vertical"
		        	android:layout_weight="1"
		        	android:background="@null"
		        	style="@style/main_style_6"
		        	android:inputType="number"
		        	android:id="@+id/complete_idc"/>
		    </LinearLayout>
		    <!-- 固定电话 -->
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:background="@drawable/publishbg"
		        android:layout_marginTop="7dp"
		        android:padding="7dp"
		        android:gravity="center_vertical">
		        <TextView
		            android:layout_width="wrap_content"
		        	android:layout_height="wrap_content"
		        	android:text="@string/yourtel"
		        	style="@style/main_style_4"/>
		        <EditText
		            android:layout_width="0dp"
		        	android:layout_height="wrap_content"
		        	android:hint="@string/select2"
		        	android:gravity="center_vertical"
		        	android:layout_weight="1"
		        	android:background="@null"
		        	style="@style/main_style_6"
		        	android:inputType="number"
		        	android:id="@+id/complete_tel"/>
		    </LinearLayout>
		    <!-- 公司名称 -->
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:background="@drawable/publishbg"
		        android:layout_marginTop="7dp"
		        android:padding="7dp"
		        android:gravity="center_vertical">
		        <TextView
		            android:layout_width="wrap_content"
		        	android:layout_height="wrap_content"
		        	android:text="@string/companyname"
		        	style="@style/main_style_4"/>
		        <EditText
		            android:layout_width="0dp"
		        	android:layout_height="wrap_content"
		        	android:hint="@string/select2"
		        	android:gravity="center_vertical"
		        	android:layout_weight="1"
		        	android:background="@null"
		        	style="@style/main_style_6"
		        	android:inputType="text"
		        	android:id="@+id/complete_company"/>
		    </LinearLayout>
		    <!-- 经营地址 -->
		    <TextView
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:layout_marginTop="7dp"
	        	android:text="@string/companyadd"
	        	style="@style/main_style_4"/>
	        <EditText
	            android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingLeft="10dp"
	        	android:background="@drawable/publishbg"
	        	android:hint="@string/select2"
	        	style="@style/main_style_6"
	        	android:inputType="text"
	        	android:id="@+id/complete_address"/>
	        <!-- 经营范围 -->
	        <TextView
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:layout_marginTop="7dp"
	        	android:text="@string/companyfw"
	        	style="@style/main_style_4"/>
	        <EditText
	            android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingLeft="10dp"
	        	android:background="@drawable/publishbg"
	        	android:hint="@string/select2"
	        	style="@style/main_style_6"
	        	android:inputType="text"
	        	android:id="@+id/complete_scope"/>
	    </LinearLayout>
    </ScrollView>
    <Button
        android:layout_width="120dp"
   		android:layout_height="40dp"
   		android:layout_gravity="center_horizontal"
   		android:layout_marginTop="10dp"
   		android:background="@drawable/btn20782"
   		style="@style/title_style"
   		android:text="@string/cerup"
   		android:onClick="onClick"
   		android:id="@+id/complete_submit"/>
</LinearLayout>
