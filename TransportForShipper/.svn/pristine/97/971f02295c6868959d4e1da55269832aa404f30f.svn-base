<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/color_background">
    <include
        layout="@layout/common_title"/>
    <LinearLayout 
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:orientation="vertical"
    	android:padding="7dp"
    	android:background="@color/main_background"
    	android:visibility="gone"
    	android:id="@+id/goods_success_info">
    	<!-- 货源信息 -->
	    <LinearLayout 
	        android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
	    	android:orientation="vertical"
	    	android:padding="7dp"
	    	android:background="@drawable/gsinfobg">
		    <LinearLayout 
		        android:layout_width="match_parent"
		    	android:layout_height="wrap_content">
		        <TextView
		            android:layout_width="0dp"
		    		android:layout_height="wrap_content"
		    		android:layout_weight="1"
		    		android:text="报价0人"
		    		style="@style/main_style_3"
		    		android:id="@+id/goods_success_count"/>
		        <TextView
		            android:layout_width="wrap_content"
		    		android:layout_height="wrap_content"
		    		android:text="2014-03-01 09:32:56"
		    		style="@style/item_goods_1"
		    		android:id="@+id/goods_success_time"/>
		    </LinearLayout>
		   	<LinearLayout 
		        android:layout_width="match_parent"
		    	android:layout_height="wrap_content"
		    	android:layout_marginTop="5dp">
		        <TextView
		            android:layout_width="wrap_content"
		    		android:layout_height="wrap_content"
		    		android:text="浙江杭州"
		    		style="@style/item_goods_2"
		    		android:id="@+id/goods_success_depart"/>
		        <ImageView
		            android:layout_width="wrap_content"
		    		android:layout_height="wrap_content"
		    		android:src="@drawable/rt"/>
		        <TextView
		            android:layout_width="wrap_content"
		    		android:layout_height="wrap_content"
		    		android:text="内蒙古呼和浩特"
		    		style="@style/item_goods_2"
		    		android:id="@+id/goods_success_target"/>
		    </LinearLayout>
			<TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_marginTop="5dp"
		   		android:text="大型机械 20吨  需9.6米 高栏车一辆"
		   		style="@style/main_style_3"
		   		android:id="@+id/goods_success_gsinfo"/>
			<TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_marginTop="5dp"
		   		android:drawableLeft="@drawable/ddtb"
		   		android:text="装货地点在杭州萧山"
		   		style="@style/main_style_3"
		   		android:id="@+id/goods_success_load"/>
		</LinearLayout>
		<!-- 运价和信息费 -->
		<RelativeLayout 
	        android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
	    	android:padding="7dp"
	    	android:background="@drawable/gsinfobg2">
			<TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:text="成交价:120000"
		   		style="@style/item_goods_3"
		   		android:id="@+id/goods_success_transprotprice"/>
			<TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_alignParentRight="true"
		   		android:text="信息费:300元"
		   		style="@style/item_goods_3"
		   		android:id="@+id/goods_success_infoprice"/>
		</RelativeLayout>
	</LinearLayout>
	<View 
	    android:layout_width="match_parent"
	    android:layout_height="3dp"
	    android:background="@drawable/line"/>
	<ImageView
	    android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:src="@drawable/qhh"/>
	<!-- 抢货人信息 -->
	<RelativeLayout
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:paddingLeft="10dp"
    	android:paddingRight="10dp"
    	android:paddingTop="15dp"
        android:background="@drawable/qhhifbg"
        android:visibility="gone"
        android:id="@+id/goods_success_driver">
        <ImageView
            android:layout_width="70dp"
        	android:layout_height="90dp"
        	android:scaleType="centerCrop"
        	android:src="@drawable/twitter_photo"
        	android:id="@+id/goods_success_photo"/>
        <TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_toRightOf="@id/goods_success_photo"
		   		android:layout_marginLeft="10dp"
		   		android:text="林大鹏"
		   		style="@style/suc_goods_1"
		   		android:id="@+id/goods_success_name"/>
        <TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_toRightOf="@id/goods_success_name"
		   		android:layout_marginLeft="10dp"
		   		android:layout_alignBaseline="@id/goods_success_name"
		   		android:text="诚信值:100"
		   		style="@style/suc_goods_2"
		   		android:id="@+id/goods_success_yslcount"/>
        <TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_toRightOf="@id/goods_success_yslcount"
		   		android:layout_marginLeft="10dp"
		   		android:layout_alignBaseline="@id/goods_success_yslcount"
		   		android:text="历史成交:8笔"
		   		style="@style/suc_goods_2"
		   		android:id="@+id/goods_success_succount"/>
        <TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_toRightOf="@id/goods_success_photo"
		   		android:layout_below="@id/goods_success_name"
		   		android:layout_marginTop="5dp"
		   		android:layout_marginLeft="10dp"
		   		android:text="承运好评度:"
		   		style="@style/suc_goods_2"
		   		android:id="@+id/goods_success_ra"/>
        <RatingBar
	   	       android:id="@+id/goods_success_ratingbar"
	   	       style="@style/MyRatingBar"
	   	       android:layout_toRightOf="@id/goods_success_ra"
		   	   android:layout_below="@id/goods_success_name"
		   	   android:layout_marginTop="7dp"
	   	       android:layout_width="wrap_content"
	   	       android:layout_height="wrap_content"
	   	       android:rating="4.0" />
        <TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_toRightOf="@id/goods_success_ratingbar"
		   		android:layout_below="@id/goods_success_name"
		   		android:layout_marginLeft="10dp"
		   		android:layout_alignBaseline="@id/goods_success_ra"
		   		android:text="523人评价"
		   		style="@style/suc_goods_2"
		   		android:id="@+id/goods_success_pjcount"/>
        <TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_toRightOf="@id/goods_success_photo"
		   		android:layout_below="@id/goods_success_ra"
		   		android:layout_marginTop="5dp"
		   		android:layout_marginLeft="10dp"
		   		android:text="公司:浙江阿凡提物流有限公司"
		   		style="@style/main_style_3"
		   		android:id="@+id/goods_success_company"/>
        <TextView
		        android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:layout_toRightOf="@id/goods_success_photo"
		   		android:layout_below="@id/goods_success_company"
		   		android:layout_marginTop="15dp"
		   		android:layout_marginLeft="10dp"
		   		android:text="手机:"
		   		style="@style/main_style_3"
		   		android:id="@+id/goods_success_mob"/>
        <TextView
            android:layout_height="40dp"
            android:layout_width="match_parent"
            android:layout_toRightOf="@id/goods_success_mob"
		   	android:layout_below="@id/goods_success_company"
		   	android:layout_marginTop="5dp"
		   	android:layout_marginLeft="10dp"
		   	android:padding="7dp"
		   	android:drawableRight="@drawable/teltb"
		   	android:background="@drawable/gsinfobg"
		   	android:text="13857441258" 
		   	android:gravity="center_vertical"
		   	style="@style/item_goods_2"
		   	android:id="@+id/goods_success_mobile"
		   	android:onClick="click"/>
   </RelativeLayout>
   <LinearLayout
        android:layout_width="match_parent"
    	android:layout_height="0dp"
    	android:layout_weight="1">
       
   </LinearLayout>
	<include
        layout="@layout/common_footer"/>
</LinearLayout>
