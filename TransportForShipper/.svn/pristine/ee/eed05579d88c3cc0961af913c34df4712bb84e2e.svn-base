<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/color_background">
    <include 
        layout="@layout/common_title"/>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical" >

                    <!-- 个人信息 -->
					<LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="143dp"
                        android:background="@drawable/qhhifbg"
                        android:orientation="vertical" >

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="113dp" 
                            android:padding="7dp">

                            <ImageView
                                android:id="@+id/picture"
                                android:layout_width="80dp"
                                android:layout_height="110dp"
                                android:src="@drawable/idc_photo" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="7dp"
                                android:layout_toRightOf="@id/picture"
                                android:orientation="vertical"
                                android:visibility="gone"
                                android:id="@+id/credit_info_1">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal" >

									<TextView
                                        android:id="@+id/realName"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="林大鹏"
                                        style="@style/suc_goods_1" />
                                    <TextView
								        android:layout_width="wrap_content"
								   		android:layout_height="wrap_content"
								   		android:layout_toRightOf="@id/realName"
								   		android:layout_marginLeft="10dp"
								   		android:layout_alignBaseline="@id/realName"
								   		android:text="诚信值:0"
								   		style="@style/suc_goods_2"
								   		android:id="@+id/yslAccount"/>
                                </RelativeLayout>

                                <TextView
                                        android:id="@+id/companyName"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="浙江杭州大鹏货运物流有限公司"
                                        style="@style/suc_goods_2"/>


                                <!-- 发货好评度开始 -->

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="22dp"
                                    android:orientation="horizontal"
                                    android:layout_marginTop="6dp" >

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="发货好评度 :"
                                        style="@style/suc_goods_2" />

                                    <!-- 评价图标 -->

                                    <RatingBar
                                        android:id="@+id/fahuoxingji"
                                        style="@style/myRatingBar"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:isIndicator="true"
                                        android:numStars="5"
                                        android:rating="1"
                                        android:clickable="false"
                                        android:stepSize="1" >
                                    </RatingBar>
									
                                    <TextView
                                        android:id="@+id/fahuoxingjicount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text=""
                                        style="@style/item_tk_2" />
                                </LinearLayout>

                                <!-- 发货好评度结束 -->


                                <!-- 承运好评度开始 -->

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="22dp"
                                    android:orientation="horizontal" >

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="承运好评度 :"
                                        style="@style/suc_goods_2" />

                                    <!-- 评价图标 -->

                                    <RatingBar
                                        android:id="@+id/chengyunxingji"
                                        style="@style/myRatingBar"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:isIndicator="true"
                                        android:numStars="5"
                                        android:clickable="false"
                                        android:rating="1" >
                                    </RatingBar>

                                    <TextView
                                        android:id="@+id/chengyunxingjicount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        style="@style/item_tk_2" />
                                </LinearLayout>

                                <!-- 承运好评度结束 -->

                            </LinearLayout>
                            
                        </RelativeLayout>
                        
						<!-- 分割线 -->

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="#EAEAEA" />
                        <!-- 等级及是否认证 -->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="25dp"
                            android:background="@drawable/gsinfobg"
                            android:gravity="center_vertical"
                            android:visibility="gone"
                            android:id="@+id/credit_info_2">
                            <!-- 等级 -->
                            <LinearLayout
                                android:layout_width="0dp"
                            	android:layout_height="wrap_content"
                            	android:layout_weight="1"
                            	android:gravity="center">
                            <ImageView
                                android:layout_width="wrap_content"
                            	android:layout_height="wrap_content"
                            	android:src="@drawable/zstb"/>
                            <TextView
                                android:layout_width="wrap_content"
                            	android:layout_height="wrap_content"
                            	android:text="未知会员"
                            	android:layout_marginLeft="3dp"
                            	style="@style/suc_goods_2"
                            	android:id="@+id/credit_level"/>
                            </LinearLayout>
                            <View
                            android:layout_width="0.5dp"
                            android:layout_height="15dp"
                            android:background="#dddddd" />
                            <!-- 是否认证 -->
                            <LinearLayout
                                android:layout_width="0dp"
                            	android:layout_height="wrap_content"
                            	android:layout_weight="1"
                            	android:gravity="center">
                            <ImageView
                                android:layout_width="wrap_content"
                            	android:layout_height="wrap_content"
                            	android:src="@drawable/yrztb"/>
                            <TextView
                                android:layout_width="wrap_content"
                            	android:layout_height="wrap_content"
                            	android:layout_marginLeft="3dp"
                            	android:text="未认证"
                            	style="@style/suc_goods_2"
                            	android:id="@+id/credit_cert"/>
                            </LinearLayout>
                            <View
                            android:layout_width="0.5dp"
                            android:layout_height="15dp"
                            android:background="#dddddd" />
                            <!-- 积分 -->
                            <LinearLayout
                                android:layout_width="0dp"
                            	android:layout_height="wrap_content"
                            	android:layout_weight="1"
                            	android:gravity="center">
                            <ImageView
                                android:layout_width="wrap_content"
                            	android:layout_height="wrap_content"
                            	android:src="@drawable/jftb"/>
                            <TextView
                                android:layout_width="wrap_content"
                            	android:layout_height="wrap_content"
                            	android:layout_marginLeft="3dp"
                            	android:text="0积分"
                            	style="@style/suc_goods_2"
                            	android:id="@+id/credit_jf"/>
                            </LinearLayout>
                        </LinearLayout>

                        <!-- 运商行提示 -->

                    </LinearLayout>

                    
                    
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="5dp" 
                        android:visibility="gone"
                        android:background="@drawable/gsinfobg">
						<LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingRight="5dp" 
                        android:paddingLeft="5dp" 
                        android:gravity="center_horizontal">
                         <ImageView
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:src="@drawable/alert" />
                         <TextView
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:text="@string/alert"
                             style="@style/item_tk_2" />
                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:text="尚未收到有效投诉，交易较安全"
                        style="@style/suc_goods_1" 
                        android:id="@+id/comment"/>
		</LinearLayout>
                    <!-- 运商行提示 结束 -->
                    


                    <!-- 个人手机号码 -->

                    <RelativeLayout
                        android:id="@+id/mobileLayout"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="7dp"
                        android:layout_marginRight="7dp"
                        android:background="@drawable/common_while_bg"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dp" 
                        android:onClick="onClick">

                        <TextView
                            android:id="@+id/shoujitext"
                            style="@style/creditResultInfo1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="手机号码" />

                        <TextView
                            android:id="@+id/mobile"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/shoujitext"
                            android:textColor="#878787"
                            android:textSize="18sp" 
                            />

                        <ImageView
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:layout_marginLeft="10dp"
                            android:layout_toRightOf="@id/mobile"
                            android:src="@drawable/teltb"
							android:id="@+id/teltb1"
							android:visibility="gone"/>
                    </RelativeLayout>

                    <!-- 个人手机号码结束 -->


                    <!-- 个人电话号码 -->

                    <RelativeLayout
                          android:id="@+id/telLayout"
                        android:layout_width="match_parent"
                        android:layout_height="47dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="7dp"
                        android:layout_marginRight="7dp"
                        android:background="@drawable/common_while_bg"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dp" 
                        android:onClick="onClick">

                        <TextView
                            android:id="@+id/shoujitext"
                            style="@style/creditResultInfo1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="固定电话" />

                        <TextView
                            android:id="@+id/tel"
                            style="@style/creditResultInfo2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/shoujitext"
                             />

                        <ImageView
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:layout_marginLeft="10dp"
                            android:layout_toRightOf="@id/tel"
                            android:src="@drawable/teltb" 
                            android:id="@+id/teltb2"
                            android:visibility="gone"/>
                    </RelativeLayout>

                    <!-- 个人电话号码结束 -->


                    <!-- 其他个人信息 -->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="342dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginLeft="7dp"
                        android:layout_marginRight="7dp"
                        android:background="@drawable/common_while_bg"
                        android:orientation="vertical"
                        android:paddingLeft="5dp" >

                        <!-- 當地排名 -->

                        <LinearLayout
                            style="@style/creditResultInfoLayout2"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" >

                            <TextView
                                style="@style/creditResultInfo1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="当地排名" />

                            <TextView
                                android:id="@+id/paiming"
                                style="@style/creditResultInfo2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />

                        </LinearLayout>

                        <!-- 分割线 -->

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#EAEAEA" />

                        <!-- 会员类型 -->

                        <LinearLayout
                            style="@style/creditResultInfoLayout2"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" >

                            <TextView
                        
                                style="@style/creditResultInfo1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="会员类型" />

                            <TextView
                                android:id="@+id/userType"
                                style="@style/creditResultInfo2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                />
                            <ImageView
                                android:id="@+id/credit_ycp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:src="@drawable/ycptb7824"
                                android:visibility="gone"/>
                        </LinearLayout>

                        <!-- 分割线 -->

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#EAEAEA" />

                        <!-- 会员级别 -->

                        <LinearLayout
                            style="@style/creditResultInfoLayout2"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" >

                            <TextView
                         
                                style="@style/creditResultInfo1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="会员级别" />

                            <TextView
                                android:id="@+id/memberLevel"
                                style="@style/creditResultInfo2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                 />
                        </LinearLayout>

                        <!-- 分割线 -->

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#EAEAEA" />


                        <!-- 精英路线 -->

                        <LinearLayout
                            style="@style/creditResultInfoLayout2"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" >

                            <TextView
                            
                                style="@style/creditResultInfo1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="经营路线" />

                            <TextView
                                android:id="@+id/companylines"
                                style="@style/creditResultInfo2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                />
                        </LinearLayout>

                        <!-- 分割线 -->

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#EAEAEA" />

                        <!-- 经营地址 -->

                        <LinearLayout
                            style="@style/creditResultInfoLayout2"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" >

                            <TextView
                         
                                style="@style/creditResultInfo1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="经营地址" />

                            <TextView
                                android:id="@+id/companyAddress"
                                style="@style/creditResultInfo2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                />
                        </LinearLayout>

                        <!-- 分割线 -->

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#EAEAEA" />
                        
                      <!-- 注册时间 -->

                        <LinearLayout
                            style="@style/creditResultInfoLayout2"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" >

                            <TextView
                               
                                style="@style/creditResultInfo1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="注册时间" />

                            <TextView
                                android:id="@+id/registerTime"
                                style="@style/creditResultInfo2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                 />
                        </LinearLayout>

                        <!-- 分割线 -->

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#EAEAEA" />
                    </LinearLayout>
                    <!-- 
                    usrid
                     -->
                   

                    <!-- 其他个人信息 结束 -->
                </LinearLayout>
        </ScrollView>

        <!-- 菜单 -->
		<LinearLayout 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:background="@color/color_background"
		    android:padding="5dp">
            <!-- 谁在查我 -->
		    <TextView
		   		android:layout_width="80dp"
		    	android:layout_height="50dp"
		    	android:padding="7dp"
		    	android:gravity="center"
		    	android:background="@drawable/footerbg1"
		   		android:drawableLeft="@drawable/szcwtb"
		   		style="@style/main_style_4"
		   		android:text="@string/queryme"
		   		android:clickable="true"
		   		android:onClick="onClick"
		   		android:id="@+id/my_szcw"/>
		    <!-- 中间按钮 -->
		    <TextView
		        android:layout_width="0dp"
		   		android:layout_height="48.5dp"
		   		android:layout_weight="1"
		   		android:layout_marginLeft="5dp"
		   		android:gravity="center"
		   		android:background="@drawable/btn20782"
		   		style="@style/title_style"
		   		android:text="@string/mycomplete"
		   		android:clickable="true"
		   		android:onClick="onClick"
		   		android:id="@+id/my_complete"/>
		    <!-- 我的评价 -->
		    <TextView
		   		android:layout_width="80dp"
		    	android:layout_height="50dp"
		    	android:layout_marginLeft="5dp"
		    	android:padding="7dp"
		    	android:gravity="center"
		    	android:background="@drawable/footerbg1"
		   		style="@style/main_style_4"
		   		android:text="@string/mypj"
		   		android:clickable="true"
		   		android:onClick="onClick"
		   		android:id="@+id/mypj"/>
		</LinearLayout>
</LinearLayout>